import java.util.scanner;
class OddEven
{
     public static void main(string[]args)  {

         int n;
        System.out.print("Enter any Number");
        Scanner r=new scanner(System.in);
        n=r.nextInt();
        
        if(n%2==0)
        {
              System.out.print("Even Number");
         }
         else
         {
              System.out.print("Odd Number");
          }
      }

